package com.mao.test.one;

import java.io.File;

public class Quest {

	public void readC() {
		File f1 = new File("c:/");
		
	}
	public void search(File f1) {
		if (f1.isFile()&&f1.exists()&&f1!=null) {
			if (f1.getAbsolutePath().contains("MySql")) {
				System.out.println(f1.getAbsolutePath());
			}
		}
	}
	
}
